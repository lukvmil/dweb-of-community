server {
    server_name 192.168.1.102;
    listen 80;

    root /home/ubuntu/dweb-of-community/frontend;

    location / {
        try_files $uri/index.html $uri.html $uri $uri/ =404;
    }

    location /api/ {
        proxy_pass http://localhost:8000;
        rewrite ^/api(/.*)$ $1 break;
    }
}